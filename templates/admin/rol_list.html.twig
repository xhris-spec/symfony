{% extends '@SonataAdmin/CRUD/base_list_field.html.twig' %}
{% block field %}
    <div>
        {% if object.rol is not empty %}
            {% for rol in object.rol %}
                {{ rol }}{% if not loop.last %}, {% endif %}
            {% endfor %}
        {% else %}
            <span>No tiene rol</span>
        {% endif %}
    </div>
{% endblock %}
